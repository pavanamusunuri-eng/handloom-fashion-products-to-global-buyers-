  </div> <!-- end grid -->

  <!-- Cart column (right) -->
  <div class="cartWrap">
    <div class="cart">
      <h3>Your Cart</h3>
      <div id="items">No items yet</div>
      <p>Total: ₹<span id="total">0</span></p>
      <button class="pay" onclick="payNow()">Pay Now</button>
    </div>

    <!-- Feedback placeholder (will be replaced with form in PART-4) -->
    <div id="feedbackSection">
      <!-- Feedback will appear HERE after cart (you selected B) -->
    </div>
  </div>

</div> <!-- end container -->

<script>
  /* Simple cart implementation that works with all parts.
     Later parts may add more features (persist, remove, edit).
  */
  let cart = [];
  let cartTotal = 0;

  function addToCart(name, price, btn) {
    // locate product card
    const card = btn.closest('.product');
    // size and qty selectors
    const sizeEl = card.querySelector('.size');
    const qtyEl = card.querySelector('.qty');

    const size = sizeEl ? sizeEl.value : 'Free Size';
    const qty = qtyEl ? parseInt(qtyEl.value) : 1;

    const lineTotal = price * qty;

    cart.push({ name, price, size, qty, lineTotal });
    cartTotal += lineTotal;
    renderCart();
  }

  function renderCart() {
    const itemsDiv = document.getElementById('items');
    if(cart.length === 0) {
      itemsDiv.innerHTML = 'No items yet';
      document.getElementById('total').textContent = '0';
      return;
    }
    let html = '';
    cart.forEach((it, idx) => {
      html += `<div class="cartItem">
                <strong>${it.name}</strong> (${it.size}) x${it.qty}
                <button class="removeBtn" onclick="removeItem(${idx})">Remove</button>
                <div>₹${it.lineTotal}</div>
               </div>`;
    });
    itemsDiv.innerHTML = html;
    document.getElementById('total').textContent = cartTotal;
  }

  function removeItem(index) {
    if(index >=0 && index < cart.length) {
      cartTotal -= cart[index].lineTotal;
      cart.splice(index,1);
      renderCart();
    }
  }

  function payNow() {
    if(cart.length === 0) {
      alert('Cart is empty!');
      return;
    }
    alert('Payment Successful!\\nAmount Paid: ₹' + cartTotal + '\\nThank you for shopping!');
    // clear cart
    cart = [];
    cartTotal = 0;
    renderCart();
    // feedback form will be shown in PART-4 (placeholder present)
  }

  // Filtering by category
  function filterCategory(cat) {
    const products = document.querySelectorAll('.product');
    products.forEach(p => {
      if(cat === 'all') {
        p.style.display = '';
      } else {
        if(p.getAttribute('data-category') === cat) p.style.display = '';
        else p.style.display = 'none';
      }
    });
  }

  // Search box filtering (searches data-name and visible category)
  function filterProducts() {
    const q = document.getElementById('search').value.toLowerCase();
    document.querySelectorAll('.product').forEach(p => {
      const name = (p.getAttribute('data-name') || '').toLowerCase();
      const catMatch = name.includes(q);
      const titleMatch = p.querySelector('p') && p.querySelector('p').innerText.toLowerCase().includes(q);
      if(q === '' || catMatch || titleMatch) p.style.display = '';
      else p.style.display = 'none';
    });
  }

  // Initialize cart UI
  renderCart();
</script>

</body>
</html>
